{
	"name": "stackoverflowextension",
	"displayName": "StackoverflowExtension",
	"description": "Better search engine for stackoverflow from inside VS Code",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:stackoverflowextension.start"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "stackoverflowextension.start",
				"title": "StackOverflow Engine"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.51.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0",
		"natural":"^2.1.5",
		"stopword":"^1.0.3",
		"zlib": "1.0.5"
	}
}
